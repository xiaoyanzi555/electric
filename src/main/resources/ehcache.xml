<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
	<!-- 缓存目录 -->
	<diskStore path="c:\\temp"/>
	<!-- 缺省的过期策略 -->
    <defaultCache
            maxElementsInMemory="10000"
            eternal="false"
            timeToIdleSeconds="120"
            timeToLiveSeconds="120"
            overflowToDisk="true"
            diskPersistent="true"
            diskExpiryThreadIntervalSeconds="120"
            memoryStoreEvictionPolicy="LRU"
            />    

</ehcache>
